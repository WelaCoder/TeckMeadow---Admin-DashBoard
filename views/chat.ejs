                    <div class="flex-grow-1">
                      <input
                        type="text"
                        class="form-control"
                        placeholder="Write your message"
                        id="m"
                        autocomplete="off"
                      />
                    </div>
                    <div class="chat-footer-buttons d-flex">
                      <button class="btn btn-primary" type="submit">
                        <i data-feather="send" class="width-15 height-15"></i>
                      </button>
                      
                    </div>
                  </form>
                </div>
                <!-- end::chat footer -->
