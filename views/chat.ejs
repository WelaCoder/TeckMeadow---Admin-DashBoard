                      <%}else{%>                      
                        <div class="m-0 small text-success">Offline</div>
                      <%}%>
                    </div>
                    
                  </div>
                </div>
                <!-- end::chat header -->

                <!-- begin::messages -->
                <div id="messages" class="messages">
                <% user.messages.forEach(message=>{%>
                <%if (message.sender==currentUser.username){%>
                    <div class="message-item me">
                <%}%>
                <%if (!(message.sender==currentUser.username)){%>
                    <div class="message-item">
                <%}%>
                    <div class="message-item-content"><%=message.message%></div>
                    <span class="time small text-muted font-italic">
                      <%if ((Date.now() - message.dated)> (1000*60*60)) {%>
                      <%=message.dated.toDateString()%>
                        
                      <%}else{%>
                      Just now
                      <%}%>
                      </span>
                  </div>
                <%})%>
                  
                      
               
                  
                </div>
                <!-- end::messages -->

