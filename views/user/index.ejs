<!--Navbar -->
<nav class="mb-1 navbar navbar-expand-lg navbar-dark bg-dark ">
        <a class="navbar-brand" href="/">YelpCamp</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent-333"
          aria-controls="navbarSupportedContent-333" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent-333">
          <% if(currentUser){ %>
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link" href="#">
                Currently Signed In As <%= currentUser.username %>
              </a>
            </li>
          </ul>
          <%} %>
          <% if(!currentUser){ %>
          <ul class="navbar-nav nav-flex-icons ml-auto">
          <%}else{ %>
          <ul class="navbar-nav nav-flex-icons">
          <% } %>
            <li class="nav-item">
              <a class="nav-link waves-effect waves-light">
                <i class="fab fa-twitter"></i>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link waves-effect waves-light">
                <i class="fab fa-google-plus-g"></i>
              </a>
            </li>
            
               
           
            
            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink-333" data-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">
                <i class="fas fa-user"></i>
              </a>
              <div class="dropdown-menu dropdown-menu-right dropdown-primary"
                aria-labelledby="navbarDropdownMenuLink-333">
                <% if(!currentUser){ %>
                    <a class="dropdown-item" href="/login"><i class="fas fa-user-alt    "></i> Login</a>
                    <a class="dropdown-item" href="/register"><i class="fas fa-user-plus    "></i> Sign Up</a>
                <%}else{ %>
                    <a class="dropdown-item" href="/logout"><i class="fas fa-sign-out-alt" aria-hidden="true"></i>Log Out  </a>
                    <a class="dropdown-item" href="/user/<%= currentUser._id %>"><i class="fas fa-sign-out-alt" aria-hidden="true"></i>View Profile  </a>
                <% }%>
              </div>
            </li>
            <!-- Dropdown -->
            
          </ul>
        </div>
      </nav>
      <div class="container">
            <%if(error && error.length>0){ %>
            <div class="alert alert-danger">
                <%=error %>
            </div>
            <% }%>
            <%if(message && message.length>0){ %>
            <div class="alert alert-success">
                <%=message %>
            </div>
            <% }%>
        </div>

      

<div class="container">
