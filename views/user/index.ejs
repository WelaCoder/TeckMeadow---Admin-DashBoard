    </div>
    </header>
    <div class="row text-center" style="display: flex;flex-wrap: wrap;">
        <% user.forEach(function(use){ %>
          <% if(!(use.username === currentUser.username)){console.log(use) %>
        <div class="col-md-3 col-sm-6">
            <div class="card mt-2 wow bounceInUp" >
                    <img class="card-img-top" src="https://pngimage.net/wp-content/uploads/2018/05/default-user-profile-image-png-6.png" alt="" >
                    <div class="card-body">
                        <h5 class="card-title"><%= use.username%></h5>
                        <a href="/user/<%=use._id%>" class="btn btn-cyan btn-sm">Chat</a>
                    </div>
            </div>       
        </div>
    <% }%>
    <% });%>
